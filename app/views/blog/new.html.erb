<h2> Add a New Blog </h2>

<%= form_with model: @blog do |form| %>
    <%= form.label :title %>
    <%= form.text_field :title %>
    <br>
    <%= form.label :content %>
    <%= form.text_field :content %>
    <br>
    <%= form.submit "Submit" %>
<% end %>

<%= link_to "Return To Main", blogs_path%>